{
  "name": "reactdemo",
  "version": "1.0.0",
  "description": "Demos with ReactJS",
  "main": "build.js",
  "devDependencies": {
    "requirejs": "^2.1.15",
    "react-tools": "^0.12.2"
  },
  "scripts": {
    "jsx": "node ./node_modules/react-tools/bin/jsx ./jsx ./js/views/components --cache-dir ./.jsx-cache -x jsx",
    "jsx-watch": "node ./node_modules/react-tools/bin/jsx ./jsx ./js/views/components --cache-dir ./.jsx-cache -x jsx --watch",
    "optimize": "node ./node_modules/requirejs/bin/r -o build.js"
  },
  "author": "Oliver Hager",
  "license": "MIT",
  "dependencies": {
    "react": "^0.12.2",
    "react-router": "^0.11.6"
  }
}
